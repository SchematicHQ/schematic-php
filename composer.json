{
  "name": "schematichq/schematic-php",
  "version": "0.0.4",
  "description": "Schematic API",
  "keywords": [
    "schematic",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://schematichq.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Papillon",
      "homepage": "https://schematichq.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Schematic\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Schematic\\Test\\": "test/"
    }
  }
}
